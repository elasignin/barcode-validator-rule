package com.ingramcontent.consolidated.labels;

import com.ingramcontent.consolidated.labels.model.BarcodeDetail;
import com.ingramcontent.consolidated.labels.model.OrderStatus;

rule "update BulkItemFeedRule"
	when 
		barcodeDetail: BarcodeDetail();
	then
		
		InventoryRule inventoryRule = bulkItemUpload.getInventoryRule();
		inventoryRule.setMinSizeToProcess(10);
		
		MPItemMatchRule mpItemMatchRule = bulkItemUpload.getMpItemMatchRule();
		mpItemMatchRule.setMinSizeToProcess(10);
		
		PriceRule priceRule = bulkItemUpload.getPriceRule();
		priceRule.setMinSizeToProcess(10);
		
	end